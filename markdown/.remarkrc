{
    "plugins": [
        "remark-preset-lint-markdown-style-guide",
        ["remark-lint-blockquote-indentation", false],
        ["remark-lint-ordered-list-marker-value", false],
        ["remark-lint-link-title-style", false],
        ["remark-lint-list-item-content-indent", false],
        ["remark-lint-list-item-spacing", false],
        ["remark-lint-list-item-indent", false],
        ["remark-lint-list-item-punctuation", false],
        ["remark-lint-maximum-line-length", false],
        ["remark-lint-no-auto-link-without-protocol", false],
        ["remark-lint-no-duplicate-headings", false],
        ["remark-lint-no-file-name-irregular-characters", false],
        ["remark-lint-no-literal-urls", false],
        ["remark-lint-no-shortcut-reference-link", false],
        ["remark-lint-no-table-indentation", false],
        ["remark-lint-table-cell-padding", false],
        ["remark-lint-table-pipe-alignment", false]
    ]
}
